{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "semi": ["error", "never"],
    "no-nested-ternary": "off",
    "func-names": "off",
    "prefer-destructuring": "off",
    "no-shadow": "off",
    "no-param-reassign":"off"
  }
}
